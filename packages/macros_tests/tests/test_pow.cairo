const TWO_TEN: u128 = pow!(2, 10);

#[test]
fn test_pow_macro() {
    assert!(TWO_TEN == 1024);
    assert!((pow!(10, 2)) == 100);
    assert!((pow!(20, 30)) == 1073741824000000000000000000000000000000_felt252);
    assert!(
        (pow!(
            2, 255,
        )) == 57896044618658097711785492504343953926634992332820282019728792003956564819968_u256,
    );
}
